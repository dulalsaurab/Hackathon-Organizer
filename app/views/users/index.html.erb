<% provide(:title, 'User List') %>
<h1>All users</h1>

<ul class="users" style="list-style-type:none;">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.user_name, user, class: "btn btn-info" %>
      <% if current_user.admin? && !current_user?(user) %>

    | <%= link_to "delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %>

      <%= link_to 'New Hackathon', new_hackathon_path %> 
      <% end %>
    </li>
    </br>
  <% end %>
</ul>
<%= will_paginate %>
